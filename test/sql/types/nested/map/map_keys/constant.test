# name: test/sql/types/nested/map/map_keys/constant.test
# group: [map_keys]

statement ok
pragma enable_verification;

# Empty
query I
select map_keys(MAP([],[]));
----
[]

# Single entry
query I
select map_keys(MAP(['a'],[5]));
----
[a]

# Multiple entries
query I
select map_keys(MAP(['a', 'b', 'c', 'd'], [5,1,8,3]))
----
[a, b, c, d]

# Null
query I
select map_keys(NULL)
----
NULL

# Map keys can not be NULL, so more extensive tests for NULL are not possible
