# name: test/sql/types/enum/test_enums_and_built_in_types.test
# description: ENUM types and built-in types
# group: [enum]

load :memory:

statement ok
PRAGMA enable_verification

statement error
CREATE TYPE "integer" AS ENUM ('1', '2', '3');
----
PLACEHOLDER_ERROR_MESSAGE671

restart

# use a type that is almost a built-in type
statement error
SELECT 4::INTEGEE
----
PLACEHOLDER_ERROR_MESSAGE670

restart

# cannot drop built-in types
statement error
DROP TYPE "INTEGER"
----
PLACEHOLDER_ERROR_MESSAGE669

statement error
DROP TYPE "INTEGEE"
----
PLACEHOLDER_ERROR_MESSAGE668

restart

# cannot create built-in type
statement error
CREATE TYPE integer AS ENUM ('1', '2', '3');
----
PLACEHOLDER_ERROR_MESSAGE667
