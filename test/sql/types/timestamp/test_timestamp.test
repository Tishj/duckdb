# name: test/sql/types/timestamp/test_timestamp.test
# description: Test TIMESTAMP type
# group: [timestamp]

statement ok
SET default_null_order='nulls_first';

statement ok
PRAGMA enable_verification

statement ok
CREATE TABLE IF NOT EXISTS timestamp (t TIMESTAMP);

statement ok
INSERT INTO timestamp VALUES ('2008-01-01 00:00:01'), (NULL), ('2007-01-01 00:00:01'), ('2008-02-01 00:00:01'), ('2008-01-02 00:00:01'), ('2008-01-01 10:00:00'), ('2008-01-01 00:10:00'), ('2008-01-01 00:00:10')

query T
SELECT timestamp '2017-07-23 13:10:11';
----
2017-07-23 13:10:11

# iso timestamps
query TT
SELECT timestamp '2017-07-23T13:10:11', timestamp '2017-07-23T13:10:11Z';
----
2017-07-23 13:10:11
2017-07-23 13:10:11

# spaces everywhere
query T
SELECT timestamp '    2017-07-23     13:10:11    ';
----
2017-07-23 13:10:11

# other trailing, preceding, or middle gunk is not accepted
statement error
SELECT timestamp '    2017-07-23     13:10:11    AA';
----
Conversion Error: timestamp field value out of range: "    2017-07-23     13:10:11    AA", expected format is (YYYY-MM-DD HH:MM:SS[.US][±HH:MM| ZONE])

statement error
SELECT timestamp 'AA2017-07-23 13:10:11';
----
Conversion Error: timestamp field value out of range: "AA2017-07-23 13:10:11", expected format is (YYYY-MM-DD HH:MM:SS[.US][±HH:MM| ZONE])

statement error
SELECT timestamp '2017-07-23A13:10:11';
----
Conversion Error: timestamp field value out of range: "2017-07-23A13:10:11", expected format is (YYYY-MM-DD HH:MM:SS[.US][±HH:MM| ZONE])

